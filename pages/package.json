{
  "name": "docs",
  "version": "1.1.2",
  "scripts": {
    "start": "concurrently \"npm:watch:*\" \"npm:serve\"",
    "start:d": "concurrently \"npm:watch:*\"",
    "build": "npm run render:pug && npm run render:md && npm run render:scss && npm run runAutoprefixer && npm run render:js && npm run copyFiles && npm run buildIndices && npm update zusty && npm run buildSassData && npm run sw",
    "sw": "workbox injectManifest b/workbox-config.js",
    "serve": "node b/browserSync",
    "watch:pug": "npm run render:pug -- -w",
    "watch:scss": "npm run render:scss -- -w",
    "watch:md": "npm run render:md -- -w",
    "watch:js": "webpack --mode=development --watch",
    "watch:toCopy": "npm run copyFiles -- -w",
    "render:pug": "node b/renderPug",
    "render:scss": "node b/renderSass",
    "render:md": "node b/renderMd",
    "render:js": "webpack --mode=production",
    "runAutoprefixer": "node b/runAutoprefixer",
    "buildIndices": "node b/buildIndices",
    "buildSassData": "node b/buildSassData",
    "copyFiles": "node b/copyFiles"
  },
  "author": "Sarsa Murmu <sarsamurmu05@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.7.3",
    "browser-sync": "^2.26.7",
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "chokidar": "^3.3.0",
    "codemirror": "^5.49.2",
    "concurrently": "^4.1.2",
    "docsearch.js": "^2.6.3",
    "fuse.js": "^3.4.6",
    "glob": "^7.1.6",
    "html-minifier": "^4.0.0",
    "markdown-it": "^10.0.0",
    "markdown-it-attrs": "^3.0.1",
    "markdown-it-container": "^2.0.0",
    "markdown-yaml-metadata-parser": "^2.0.3",
    "node-sass": "^4.13.0",
    "postcss": "^7.0.24",
    "prismjs": "^1.17.1",
    "pug": "^2.0.4",
    "sass.js": "^0.11.1",
    "terser-webpack-plugin": "^2.2.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "workbox-cli": "^4.3.1",
    "workbox-window": "^4.3.1",
    "zusty": "^1.1.3"
  }
}
