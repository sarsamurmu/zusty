{
  "name": "zusty",
  "version": "0.0.1",
  "description": "A Powerful CSS Framework",
  "author": "Sarsa Murmu <sarsamurmu05@gmail.com>",
  "keywords": [
    "css",
    "framework",
    "scss",
    "html5"
  ],
  "main": "zusty.scss",
  "scripts": {
    "build": "npm run build-css && npm run add-autoprefix && npm run minify-css",
    "build-parted": "npm run build-css-p-theme && npm run build-css-p-values && npm run build-css-p-zusty && npm run add-autoprefix-p && npm run minify-css-p",
    "add-autoprefix": "postcss --use autoprefixer --map false --output dist/zusty.css dist/zusty.css",
    "add-autoprefix-p": "postcss --use autoprefixer --map false --output dist/parted/zusty.css dist/parted/zusty.css",
    "build-css": "node-sass --output-style expanded --source-map true zusty.scss dist/zusty.css",
    "build-css-p-theme": "node-sass --output-style expanded --source-map true scss/parted/theme.scss dist/parted/theme.css",
    "build-css-p-values": "node-sass --output-style expanded --source-map true scss/parted/values.scss dist/parted/values.css",
    "build-css-p-zusty": "node-sass --output-style expanded --source-map true scss/parted/zusty.scss dist/parted/zusty.css",
    "minify-css": "cleancss -o dist/zusty.min.css dist/zusty.css",
    "minify-css-p": "cleancss -o dist/parted/zusty.min.css dist/parted/zusty.css",
    "start": "npm run build-css -- --watch",
    "test": "npm run build-css"
  },
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "clean-css-cli": "^4.3.0",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.2"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 10 Explorer versions",
    "last 10 iOS versions",
    "last 10 Opera versions",
    "last 10 Safari versions",
    "last 10 Android versions",
    "last 10 Edge versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarsamurmu/zusty.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sarsamurmu/zusty/issues",
    "email": "sarsamurmu05@gmail.com"
  },
  "homepage": "https://github.com/sarsamurmu/zusty#readme"
}
